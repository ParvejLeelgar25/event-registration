<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://axelor.com/xml/ns/object-views
  http://axelor.com/xml/ns/object-views/object-views_5.0.xsd">
  
  <grid model="com.axelor.event.registration.db.Address" title="Address" name="address-grid">
  	<field name="fullName"/>
  </grid>
  
  <form model="com.axelor.event.registration.db.Address" title="Address" name="address-form">
  	<panel name="address-panel" title="Address">
  		<field name="flat"/>
  		<field name="street"/>
  		<field name="landMark"/>
  		<field name="city"/>
  		<field name="country"/>
  		<field name="fullName"/>
  	</panel>
  </form>
  
  </object-views>